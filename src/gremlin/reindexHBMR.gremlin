// ReIndex HBase using MR
// ======================

graph.tx().rollback()

// Reindex existing data
mgmt = graph.openManagement()
mr = new org.janusgraph.hadoop.MapReduceIndexManagement(graph)
mr.updateIndex(mgmt.getGraphIndex('byLbl'    ), SchemaAction.REINDEX).get()
mr.updateIndex(mgmt.getGraphIndex('byTitle'  ), SchemaAction.REINDEX).get()
mr.updateIndex(mgmt.getGraphIndex('byName'   ), SchemaAction.REINDEX).get()
mgmt.getGraphIndexes(Vertex.class)
mgmt.commit()