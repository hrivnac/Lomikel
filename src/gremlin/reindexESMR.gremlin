// ReIndex ElasticSearch using MR
// ==============================

graph.tx().rollback()

// Reindex existing data
mgmt = graph.openManagement()
mr = new org.janusgraph.hadoop.MapReduceIndexManagement(graph)
mr.updateIndex(mgmt.getGraphIndex('byLblES'  ), SchemaAction.REINDEX).get()
mr.updateIndex(mgmt.getGraphIndex('byTitleES'), SchemaAction.REINDEX).get()
mr.updateIndex(mgmt.getGraphIndex('byNameES' ), SchemaAction.REINDEX).get()
mgmt.getGraphIndexes(Vertex.class)
mgmt.commit()
